<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . index.html [L]
  RewriteCond %{SERVER_PORT} 80
  Header set Strict-Transport-Security "max-age=31536000" env=HTTPS
</IfModule>
